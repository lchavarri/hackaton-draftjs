{
  "name": "draftjs-demo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "parcel src/index.html --out-dir build/debug --port 3000",
    "build-parcel": "parcel build src/index.ts --no-content-hash --no-source-maps --out-dir build/release --public-url ./release",
    "build-quip": "create-quip-app pack ./build",
    "build": "npm run build-parcel && npm run build-quip"
  },
  "devDependencies": {
    "@types/node": "^12.12.8",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "create-quip-app": "0.0.45",
    "parcel-bundler": "^1.12.4",
    "quip-apps-api": "^1.0.0-alpha.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass": "^1.23.6",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@types/draft-js": "^0.10.37",
    "draft-js": "^0.11.3",
    "quip": "^0.1.2"
  },
  "description": "Hackaton app"
}
